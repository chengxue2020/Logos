function getPlayer(playerConfig, mediaInfo) {
  try {
    kalturaPlayer = KalturaPlayer.setup(playerConfig);
    kalturaPlayer.loadMedia(mediaInfo);
    return kalturaPlayer;
  } catch (e) {
    console.error(e.message)
  }
}

