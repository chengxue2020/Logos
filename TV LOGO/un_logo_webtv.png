// Screen Aspect ratio changes what needs to be hidden or not
var screenRatio = jQuery(window).width() / jQuery(window).height();
var screenWidth = jQuery(window).width()
jQuery(window).resize(function () {
  screenRatio = jQuery(window).width() / jQuery(window).height();
  screenWidth = jQuery(window).width()
  checkNav()
});

function checkNav() {
  if (screenWidth < 992) {
    if (screenRatio < 1) {
      if (screenWidth > 720) {
        jQuery('.brandbar-header').show()
      } else {
        jQuery('.brandbar-header').hide()
      }
      jQuery('.navbar').show()
    } else {
      jQuery('.brandbar-header').hide()
      jQuery('.navbar').hide()
    }
  }
}

function showNav() {
  if ((screenRatio >= 1) || (screenWidth >= 992)) {
    jQuery('.brandbar-header').fadeIn()
    jQuery('.navbar').fadeIn()
  }
}

function hideNav() {
  if ((screenRatio >= 1) || (screenWidth >= 992)) {
    jQuery('.brandbar-header').fadeOut()
    jQuery('.navbar').fadeOut()
  }
}


jQuery(document).ready(function() {
  if (mediaScheduled) {
    showNav();
    jQuery('#un3-video-overlay .un3-video-overlay-content').removeAttr("style");
  }
})
if (!mediaScheduled) {
// Menu appears on mouse move
var timeout = null;

jQuery(document).on('mousemove', function () {
  clearTimeout(timeout);
  if (!((screenRatio >= 1) && (screenWidth < 992))) {
    showNav()
  }

  timeout = setTimeout(function () {
    var scroll = jQuery(window).scrollTop();
    if ((scroll == 0) && (!player.paused)) {
      hideNav()
    }
  }, 2000);
});

// Menu appears on scroll
jQuery(window).scroll(function (event) {
  var scroll = jQuery(window).scrollTop();
  if (scroll == 0) {
    if (!player.paused) {
      hideNav()
    }
  } else {
    showNav()
  }
});


