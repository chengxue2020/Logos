    function getawalkuliah(tanggal) {
        const gel_1 = new Date(tanggal);
        const ye = new Intl.DateTimeFormat('ID', {
            year: 'numeric'
        }).format(gel_1)
        const mo = new Intl.DateTimeFormat('ID', {
            month: 'short'
        }).format(gel_1)
        const da = new Intl.DateTimeFormat('ID', {
            day: '2-digit'
        }).format(gel_1)

        return da + '-' + mo + '-' + ye;
    }


